{
  "name": "csnp",
  "type": "module",
  "version": "1.3.8",
  "packageManager": "pnpm@9.5.0",
  "author": "Russell Xio <junxio220@gmail.com>",
  "description": "A cli-tool for generating VSCode code snippets.",
  "homepage": "https://github.com/jxzho/csnp#csnp",
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "tsx watch src/scripts/start.ts",
    "test": "jest",
    "check": "tsc --noEmit && jest",
    "build": "rimraf dist && pnpm check && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxzho/csnp.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.6.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.5.9",
    "@types/prompts": "^2.4.4",
    "@types/react": "^18.3.3",
    "babel-jest": "^29.6.4",
    "comment-json": "^4.2.3",
    "gray-matter": "^4.0.3",
    "ink": "^5.0.1",
    "jest": "^29.4.1",
    "kolorist": "^1.6.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "yoga-wasm-web": "^0.3.3"
  },
  "bin": {
    "csnp": "bin/entry.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.20.3"
  },
  "keywords": [
    "csnp",
    "Code Snippets",
    "VSCode Snippets"
  ]
}
