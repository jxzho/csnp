{
  "name": "csnp",
  "type": "module",
  "version": "1.2.5",
  "packageManager": "pnpm@8.7.1",
  "author": "Russell Xio <junxio220@gmail.com>",
  "description": "Generate vscode code snippet",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "csnp": "bin/entry.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "jiti src/scripts/start.ts",
    "test": "jest",
    "check": "tsc --noEmit && jest",
    "build": "unbuild"
  },
  "license": "MIT",
  "homepage": "https://github.com/jxzho/csnp#csnp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxzho/csnp.git"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.6.4",
    "@types/jest": "^29.5.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.5.9",
    "@types/prompts": "^2.4.4",
    "babel-jest": "^29.6.4",
    "comment-json": "^4.2.3",
    "gray-matter": "^4.0.3",
    "jest": "^29.4.1",
    "jiti": "^1.20.0",
    "kolorist": "^1.6.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "keywords": [
    "csnp",
    "Code Snippets",
    "VS Code Snippets"
  ]
}
